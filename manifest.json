{

    "manifest_version": 3,
    "name": "My Extension",
    "version": "1.0.1",
    "description": "Replace YouTube ads with a mini game.",

    "content_scripts": [
        {
        "matches": ["https://www.youtube.com/*"],
        "js": ["game.js"]
        }
    ],

    "web_accessible_resources": [
        {
        "matches": ["<all_urls>"],
        "resources": ["*", "**/*"]
        }
    ]
}